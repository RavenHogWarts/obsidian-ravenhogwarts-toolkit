{
	"name": "obsidian-ravenhogwarts-toolkit",
	"version": "1.0.0",
	"description": "A personal toolkit plugin (OTK) that integrates various Obsidian utilities, designed to enhance personal note-taking experience.",
	"main": "main.js",
	"scripts": {
		"dev": "node scripts/esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node scripts/esbuild.config.mjs production",
		"version": "node scripts/version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint . --ext .ts,.tsx",
		"lint:fix": "eslint . --ext .ts,.tsx --fix"
	},
	"keywords": [
		"OTK",
		"Obsidian Toolkit",
		"Obsidian RavenHogWarts Toolkit"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"@typescript-eslint/eslint-plugin": "^5.29.0",
		"@typescript-eslint/parser": "^5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"eslint": "^8.57.1",
		"monaco-editor": "^0.52.2",
		"obsidian": "latest",
		"postcss": "^8.5.1",
		"postcss-nesting": "^13.0.1",
		"tslib": "2.4.0",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"@emotion/react": "^11.11.3",
		"@emotion/styled": "^11.11.0",
		"@floating-ui/react": "^0.26.28",
		"@types/uuid": "^10.0.0",
		"ag-grid-community": "^30.2.0",
		"ag-grid-react": "^30.2.0",
		"date-fns": "^4.1.0",
		"fs-extra": "^11.3.0",
		"html-react-parser": "^5.2.2",
		"lucide-react": "^0.460.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-select": "^5.8.3",
		"uuid": "^11.0.3"
	}
}
